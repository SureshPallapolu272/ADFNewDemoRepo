{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AzureMorningDatafactory"
		},
		"LS_ADLSConnect_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLSConnect'"
		},
		"LS_BlobConnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_BlobConnection'"
		},
		"ls_adlsconnection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adlsconnection'"
		},
		"LS_ADLSConnect_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfl200adlsgen2.dfs.core.windows.net"
		},
		"LS_AKV_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://azurekeyvaultdemo001.vault.azure.net/"
		},
		"LS_BlobConnect_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "AccountKey"
		},
		"LS_HTTPConnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().baseURL}"
		},
		"LS_SQLConnection_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "SQLConnectionString"
		},
		"ls_adlsconnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfl200newadls.dfs.core.windows.net"
		},
		"Tr_EventTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/0b09d9a2-10b0-4bdb-966c-fa3ebfa3ba0a/resourceGroups/NewAzureresourceGroup01/providers/Microsoft.Storage/storageAccounts/adfl200morningsa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AzureMorningDatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLSConnect_accountKey": {
						"value": "[parameters('LS_ADLSConnect_accountKey')]"
					},
					"LS_BlobConnection_connectionString": {
						"value": "[parameters('LS_BlobConnection_connectionString')]"
					},
					"ls_adlsconnection_accountKey": {
						"value": "[parameters('ls_adlsconnection_accountKey')]"
					},
					"LS_ADLSConnect_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLSConnect_properties_typeProperties_url')]"
					},
					"LS_AKV_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_AKV_properties_typeProperties_baseUrl')]"
					},
					"LS_HTTPConnection_properties_typeProperties_url": {
						"value": "[parameters('LS_HTTPConnection_properties_typeProperties_url')]"
					},
					"ls_adlsconnection_properties_typeProperties_url": {
						"value": "[parameters('ls_adlsconnection_properties_typeProperties_url')]"
					},
					"LS_BlobConnect_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_BlobConnect_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_SQLConnection_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_SQLConnection_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "AzureMorningDatafactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'AzureMorningDatafactory_ArmTemplate_0')]"
			]
		},
		{
			"name": "AzureMorningDatafactory_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Tr_EventTrigger_properties_typeProperties_scope": {
						"value": "[parameters('Tr_EventTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'AzureMorningDatafactory_ArmTemplate_1')]"
			]
		}
	]
}